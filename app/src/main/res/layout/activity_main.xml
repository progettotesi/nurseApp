        tools:layout_editor_absoluteY="347dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/cartelle"
        tools:layout_editor_absoluteY="323dp"
