<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context="russodiego.nurseapp.NurseActivity">

    <TextView
        android:id="@+id/txt_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/background_light"
        android:text="Nurse Profile"
        android:textAllCaps="true"
        android:textColor="000"
        android:textSize="30sp"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.103"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/img_profile"
        android:layout_width="135dp"
        android:layout_height="120dp"
        android:background="@android:color/background_light"
        android:contentDescription=""
        android:src="@drawable/profile"
        app:srcCompat="@drawable/profile"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="145dp" />

    <ImageButton
        android:id="@+id/img_samples"
        android:layout_width="135dp"
        android:layout_height="120dp"
        android:background="@android:color/background_light"
        app:srcCompat="@drawable/samples"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="145dp" />

    <ImageButton
        android:id="@+id/img_folders"
        android:layout_width="135dp"
        android:layout_height="120dp"
        android:background="@android:color/background_light"
        app:srcCompat="@drawable/folders"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="285dp" />

    <ImageButton
        android:id="@+id/img_drugs"
        android:layout_width="135dp"
        android:layout_height="120dp"
        android:background="@android:color/background_light"
        android:contentDescription=""
        app:srcCompat="@drawable/drugs"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="285dp" />

</android.support.constraint.ConstraintLayout>
